@model IEnumerable<Trials.Run>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Faults)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Track.Desc)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Faults)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Track.Desc)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.RunId }) |
            @Html.ActionLink("Details", "Details", new { id=item.RunId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.RunId })
        </td>
    </tr>
}

</table>
